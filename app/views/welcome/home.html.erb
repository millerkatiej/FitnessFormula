 <div>
  <% if user_signed_in? %>
    <div id="corner_signout"><%= current_user.email %> |     <%= link_to "Sign Out", destroy_user_session_path, :method => :delete %></div>
    <form method="get" action="exercises/new"><button type="submit">Add Exercise</button></form>
    <h2>My Exercises</h2>
    <table style="width:1000px">
      <tr>
        <th>Exercise</th>
        <th>Time (in minutes)</th> 
        <th>Reps/miles</th>
        <th>Exhaustion Level (out of 10)</th>
        <th>Date (mm/dd/yy)</th>
      </tr>
      <tr>
        <td></td>
        <td></td> 
        <td></td>
        <td></td>
        <td></td>
      </tr>
    </table>
  <% else %>
    <%= link_to "Sign Up", new_user_registration_path %> or <%= link_to "Sign In", new_user_session_path %>
  <% end %>      
</div>